[access "refs/heads/*"]
abandon = group Change Owner
abandon = group ops-docs-maintainers
label-Code-Review = -2..+2 group ops-docs-maintainers
label-Workflow = -1..+1 group infra-core

[submit]
mergeContent = true
action = rebase if necessary
[access "refs/tags/*"]
create = group ops-docs-maintainers
push = +force group ops-docs-maintainers
pushTag = +force group ops-docs-maintainers
pushSignedTag = group ops-docs-maintainers
